<div class="signups">
  <%= link_to "Dashboard", root_path %>
</div>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Image</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user.pictures.each do |picture| %>
      <tr>
        <td><%= picture.title %></td>
        <% if picture.image.medium.url %>
          <td><%= image_tag picture.image.medium.url %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
